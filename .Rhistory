iso_map <- iso_map +
tm_title("Driving Distance to Closest Major League Baseball Stadium")
iso_map <- iso_map +
tm_credits(text = "Author: Elizabeth C. Delmelle\n@weizman_MUSA\nTravel calculations by MapBox",
position = c("left", "bottom"))
iso_map
