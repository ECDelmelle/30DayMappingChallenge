tm_polygons(fill = "hours", col = NA, fill.legend = tm_legend(title = "Hours", orientation = "landscape"))+
tm_layout(frame = FALSE, legend.frame = FALSE)
iso_map <- iso_map + tm_shape(cont_us)+
tm_polygons(fill = NA, col = "white", lwd = 0.75)
